<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.springboot.vue.mapper.StudentsMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.springboot.vue.entity.Students">
		<id column="stu_id" property="stuId" />
		<result column="stu_name" property="stuName" />
		<result column="stu_sex" property="stuSex" />
		<result column="stu_age" property="stuAge" />
		<result column="stu_info" property="stuInfo" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        stu_id AS stuId, stu_name AS stuName, stu_sex AS stuSex, stu_age AS stuAge, stu_info AS stuInfo
    </sql>

</mapper>
